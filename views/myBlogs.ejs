<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Website</title>
    <link rel="stylesheet" href="css/blogs.css" />
  </head>
  <body>
    <%- include("component/navbar") %>
    <h1>My Blogs</h1>
    <div class="container">
      <%myBlogs.length > 0 && myBlogs.forEach((blog)=>{ %>
      <div class="card">
        <h2><%- blog.title %></h2>
        <h3><%- blog.subTitle %></h3>
        <p><%- blog.description %></p>
        <img src="<%- blog.image %>" alt="" height="200px" width="200px" >
        <a href="/single/<%-blog.id %>">Read More</a>
      </div>
      <% }) %>
    </div>
    <%- include('component/footer') %>
  </body>
</html>
